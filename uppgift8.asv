clear

d = 75; % Distans mellan fordon för att sakta ner
vmax = 25; % Högsta tillåtna hastighet
v = 5; % Hastigheten för första bilen

t0 = 0; % starttid
tend = 40; % sluttid
N = 100; % antal steg
h = (tend-t0)/N; % steglängd
M = 10; % antal bilar
t = t0:h:tend; %



x = zeros(N, M);

x0 = zeros(1,M);
x(1, :) = x0;

%Beräkna bil N
f = @(t,x) (5);
for k=1_
  for j=1:(M-1)
    x(k,M-j) = x(k,M-j) + h*speed1(x(k,M-j+1)-x(k,M-j), vmax); % Eulers metod
  end
end

plot(t, x)