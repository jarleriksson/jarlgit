%Beräkna bil 1
v = 32;
for k=1:N
  x(k+1,1) = x(k,1) + h*v; % Eulers metod
end

%Beräkna bil N
f = @(t,x) (5);
for k=1:N
  for j=2:n
    x(k+1,j) = x(k,j) + h*speed1(, d, vmax); % Eulers metod
  end
end

